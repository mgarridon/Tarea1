<div class="box">
  <div class="box-header">
    <h1 class="box-title"><b>Cliente</b></h1>
    <div class="right_col" role="main">
      <div class="row">
        <div class="row">
          <div class="col-md-12">
            <div class="x_panel">


              <div class="x_content">
                <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
                <script src="http://getbootstrap.com/dist/js/bootstrap.min.js"></script>

                <div class="container">
                  <div class="row">


                    <div class="container">
                      <div class="row">

                        <div class="col-lg-6 col-sm-6 col-md-6 col-xs-12">
                          <div class="form-group"></div>
                          <label for="nombre">Rut : </label>
                          <%= @cliente.rut %><br>
                          <label for=precio>Nombre :</label>
                          <%= @cliente.nombre %><br>
                          <label for=precio>Apellido:</label>
                          <%= @cliente.apellido %><br>
                          <label for=marca>Dirección : </label>
                          <%= @cliente.direccion %><br>
                          <label for=descripcion>Teléfono : </label>
                          <%= @cliente.telefono %><br><br>
                          <%= link_to 'Atrás', clientes_path , class:'btn btn-danger' %>
                        </div>


                      </div>

                    </div>

                  </div>

                </div>


              </div>
            </div>



            <!-- /.modal-dialog -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

